@model UniStore.Models.ViewModels.User.UserFullVM

@{
    ViewBag.Title = "Delete";
}

<h2>User details</h2>

<div>
    <hr/>

    @Html.Partial("DetailsPartial", Model)

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="offset-1">
            @Html.ActionLink("Edit", "Edit", new { username = Model.UserName }, new { @class = "btn btn-primary" })
            @Html.ActionLink("Back to List", "All", null, new { @class = "btn btn-success" })
        </div>
    }
</div>